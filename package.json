{
  "name": "tractors",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      ".+\\.(css|scss|png|jpg|svg)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest",
      "\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.ts",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.ts"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@react-leaflet/core": "^2.1.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "antd": "^5.1.4",
    "antd-img-crop": "^4.6.0",
    "axios": "^1.3.3",
    "easy-bem": "^1.1.1",
    "install-peerdeps": "^3.0.3",
    "jest-environment-jsdom": "^29.4.0",
    "jest-transform-stub": "^2.0.0",
    "leaflet": "^1.9.3",
    "moment": "^2.29.4",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-input-mask": "3.0.0-alpha.2",
    "react-leaflet": "^4.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.6.2",
    "react-router-dom": "^6.6.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.(js|jsx)": [
      "yarn run lint:write",
      "yarn run prettier"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.5",
    "@types/leaflet": "^1.9.0",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "global-jsdom": "^8.7.0",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.5"
  }
}
